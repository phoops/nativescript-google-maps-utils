{
  "name": "nativescript-google-maps-utils",
  "version": "0.0.1",
  "description": "A NativeScript Goole maps utils for IOS & Android.",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "4.0.0",
      "android": "4.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phoops/nativescript-google-maps-utils.git"
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json",
    "ngc": "ngc --project tsconfig.aot.json",
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android",
    "demo-ng-ios": "grunt build && cd demo-ng && tns run ios",
    "demo-ng-android": "grunt build && cd demo-ng && tns run android"
  },
  "keywords": [
    "NativeScript",
    "Google maps utils",
    "Cluster",
    "Clustering"
  ],
  "author": "Emiliano Biscardi <emiliano.biscardi@phoops.it>",
  "contributors": [
    "Nader Toukabri (https://github.com/naderio)"
  ],
  "license": "Apache-2.0",
  "peerDependencies": {
    "@nativescript/core": ">= 7"
  },
  "devDependencies": {
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@nativescript/core": "^7.3.0",
    "@nativescript/types": "^7.3.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "nativescript-angular": "^5.3.0",
    "rxjs": "~5.5.6",
    "ts-patch": "^1.3.3",
    "tslint": "^6.1.3",
    "typescript": "3.9.7",
    "zone.js": "~0.8.12"
  }
}
